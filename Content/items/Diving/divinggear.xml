<?xml version="1.0" encoding="utf-8"?>
<Override>
  <Items>
    <Item identifier="divingmask">
      <inherit identifier="divingmask">
        <doinherit/>
        <replace sel="./Wearable[@limbtype='Head']/sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite/@sourcerect">
          206,318,84,80
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite/@origin">
          0.52,0.4
        </replace>
      </inherit>
    </Item>
    <Item identifier="divingsuit">
      <inherit identifier="divingsuit">
        <doinherit/>
        <!-- item sprite on ground -->
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Diving/DivingSuit_Item.png
        </replace>
        <!-- contained sprite, Vertical Locker -->
        <replace sel="./ContainedSprite[@allowedcontainertags='divingsuitcontainervertical']/@texture">
          %ModDir%/Content/Items/Diving/DivingSuit_Item.png
        </replace>
        <!-- contained sprite, Horizontal Locker -->
        <replace sel="./ContainedSprite[@allowedcontainertags='divingsuitcontainerhorizontal']/@texture">
          %ModDir%/Content/Items/Diving/DivingSuit_Item.png
        </replace>
        <!-- worn clothing sprites -->
        <!-- replace all first, then replace the head that stands out -->
        <replace sel="./Wearable/sprite/@texture">
          reg_DivingSuit.png
        </replace>
        <!-- head -->
        <replace sel="./Wearable/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Head']/@origin">
          0.5,0.56796
        </replace>
        <del sel="./Wearable/StatusEffect/Sound"/>
        <add sel="./Wearable/StatusEffect">
          <Sound file="%ModDir%/Content/Items/Diving/DivingSuitLoop1.ogg" range="500" loop="true" />
          <Sound file="%ModDir%/Content/Items/Diving/DivingSuitLoop2.ogg" range="500" loop="true" />
        </add>
      </inherit>
    </Item>
    <Item identifier="underwaterscooter">
      <inherit identifier="underwaterscooter">
        <doinherit/>
        <replace sel="./Sprite/@texture">
          Scooter.png
        </replace>
        <replace sel="./Propulsion/sound[@type='OnUse']/@file">
          %ModDir%/Content/Items/Diving/ScooterLoop.ogg
        </replace>
      </inherit>
    </Item>
    <Item identifier="abyssdivingsuit">
      <inherit identifier="abyssdivingsuit">
