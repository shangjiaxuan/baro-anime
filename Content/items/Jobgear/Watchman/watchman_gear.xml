<?xml version="1.0" encoding="utf-8"?>
<Items>
  <Override>
    <Item identifier="watchmanclothes">
      <inherit identifier="watchmanclothes">
        <doinherit/>
        <replace sel="./Wearable/sprite/@texture">watchman.png</replace>
        <replace sel="./Wearable/sprite/@hidelimb">false</replace>
        <replace sel="./Wearable/sprite[@limb='RightLeg' or @limb='LeftLeg' or @limb='LeftFoot' or @limb='RightFoot']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
  </Override>
</Items>
