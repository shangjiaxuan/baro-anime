      <inherit identifier="autoinjectorheadset">
        <doinherit/>
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/JobGear/TalentGear.png
        </replace>
        <replace sel="./InventoryIcon/@texture">
          %ModDir%/Content/Items/JobGear/TalentGear.png
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/JobGear/TalentGear.png
        </replace>
      </inherit>
    </Item>
    <Item identifier="advancedgenesplicer">
