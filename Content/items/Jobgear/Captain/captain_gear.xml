<?xml version="1.0" encoding="utf-8"?>
<Items>
  <Override>
    <!-- HATS START -->
    <Item identifier="captainscap1">
      <inherit identifier="captainscap1">
        <doinherit/>
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Sprite/@sourcerect">
          11,124,91,46
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@sourcerect">
          11,124,91,46
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@origin">
          0.483,1.237
        </replace>
        <!-- Show hair -->
        <del sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@hidewearablesoftype"/>
      </inherit>
    </Item>
    <Item identifier="captainscap2">
      <inherit identifier="captainscap2">
        <doinherit/>
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Sprite/@sourcerect">
          209,124,91,46
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@sourcerect">
          209,124,91,46
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@origin">
          0.483,1.237
        </replace>
        <del sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@hidewearablesoftype"/>
      </inherit>
    </Item>
    <Item identifier="captainscap3">
      <inherit identifier="captainscap3">
        <doinherit/>
        <replace sel="./Sprite/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Sprite/@sourcerect">
          110,116,90,62
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@texture">
          %ModDir%/Content/Items/Jobgear/headgears.png
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@sourcerect">
          110,116,90,62
        </replace>
        <replace sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@origin">
          0.58,1.1
        </replace>
        <!-- Show hair -->
        <del sel="./Wearable[@limbtype='Head']/sprite[@limb='Head']/@hidewearablesoftype"/>
      </inherit>
    </Item>
    <!-- HATS END -->

    <!-- UNIFORMS START -->
    <Item identifier="captainsuniform1">
      <inherit identifier="captainsuniform1">
        <doinherit/>
        <replace sel="./Wearable/sprite/@texture">
          captain_1.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Torso' or @limb='RightArm' or @limb='LeftArm' or @limb='RightThigh' or @limb='LeftThigh']/@hidelimb">
          false
        </replace>
      </inherit>
    </Item>
    <Item identifier="captainsuniform2">
      <inherit identifier="captainsuniform2">
        <doinherit/>
        <replace sel="./Wearable/sprite/@texture">
          captain_2.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='Torso' or @limb='RightArm' or @limb='LeftArm' or @limb='RightArm' or @limb='LeftArm' or @limb='RightThigh' or @limb='LeftThigh']/@hidelimb">
          false
        </replace>
      </inherit>
    </Item>
    <Item identifier="captainsuniform3">
      <inherit identifier="captainsuniform3">
        <doinherit/>
        <replace sel="./Wearable/sprite/@texture">
          captain_3.png
        </replace>
        <del sel="./Wearable/sprite"/>
        <add sel="./Wearable">
          <sprite name="Captain's Uniform 3 Torso" texture="captain_3.png" limb="Torso" hidelimb="false" inherittexturescale="true" inheritorigin="false" inheritsourcerect="false" sourcerect="155,0,117,216" origin="0.5214,0.4444" />
          <sprite name="Captain's Uniform 3 Right Hand" texture="captain_3.png" limb="RightHand" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Left Hand" texture="captain_3.png" limb="LeftHand" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Right Lower Arm" texture="captain_3.png" limb="RightArm" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Left Lower Arm" texture="captain_3.png" limb="LeftArm" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Right Upper Arm" texture="captain_3.png" limb="RightForearm" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Left Upper Arm" texture="captain_3.png" limb="LeftForearm" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Waist" texture="captain_3.png" limb="Waist" hidelimb="true" inherittexturescale="true" inheritorigin="false" inheritsourcerect="false" sourcerect="160,216,112,64" origin="0.5,0.5" />
          <sprite name="Captain's Uniform 3 Right Thigh" texture="captain_3.png" limb="RightThigh" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Left Thigh" texture="captain_3.png" limb="LeftThigh" hidelimb="false" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Right Leg" texture="captain_3.png" limb="RightLeg" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="false" sourcerect="0,128,80,128" />
          <sprite name="Captain's Uniform 3 Left Leg" texture="captain_3.png" limb="LeftLeg" hidelimb="true" inherittexturescale="true" inheritorigin="false" inheritsourcerect="false" sourcerect="87,128,66,128" origin="0.5,0.5" />
          <sprite name="Captain's Uniform 3 Left Shoe" texture="captain_3.png" limb="LeftFoot" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
          <sprite name="Captain's Uniform 3 Right Shoe" texture="captain_3.png" limb="RightFoot" hidelimb="true" inherittexturescale="true" inheritorigin="true" inheritsourcerect="true" />
        </add>
      </inherit>
    </Item>
    <!-- UNIFORMS END -->
  </Override>
</Items>
