<?xml version="1.0" encoding="utf-8"?>
<Items>
  <Override>
    <Item identifier="orangejumpsuit1">
      <inherit identifier="orangejumpsuit1">
        <doinherit/>
        <replace sel="./Wearable/sprite/@texture">
          engineer_1.png
        </replace>
        <replace sel="./Wearable/sprite[@limb=RightThigh or @limb=LeftThigh]/@hidelimb">
          false
        </replace>
      </inherit>
    </Item>
    <Item identifier="orangejumpsuit2">
      <inherit identifier="orangejumpsuit2">
        <doinherit/>
        <replace sel="./Wearable/sprite/@texture">
          engineer_2.png
        </replace>
        <replace sel="./Wearable/sprite[@limb='RightThigh' or @limb='LeftThigh' or @limb='RightLeg' or @limb='LeftLeg']/@hidelimb">
          false
        </replace>
      </inherit>
    </Item>
  </Override>
</Items>
