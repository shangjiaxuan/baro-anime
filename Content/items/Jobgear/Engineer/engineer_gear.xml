<?xml version="1.0" encoding="utf-8"?>
<Items>
  <Override>
    <Item identifier="orangejumpsuit1">
      <inherit identifier="orangejumpsuit1">
        <doinherit/>
        <del sel="./InventoryIcon"/>
        <add sel=".">
          <InventoryIcon name="Engineer Uniform 1 Icon" texture="%ModDir%/Content/Items/Jobgear/OutfitIcons.png" sourcerect="0,99,128,99" origin="0.5,0.5" />
        </add>
        <replace sel="./Wearable/sprite/@texture">engineer_1.png</replace>
        <replace sel="./Wearable/sprite/@hidelimb">false</replace>
        <replace sel="./Wearable/sprite[@limb='RightHand' or @limb='LeftHand' or @limb='RightForearm' or @limb='LeftForearm' or @limb='Waist' or @limb='LeftFoot' or @limb='RightFoot']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
    <Item identifier="orangejumpsuit2">
      <inherit identifier="orangejumpsuit2">
        <doinherit/>
        <del sel="./InventoryIcon"/>
        <add sel=".">
          <InventoryIcon name="Engineer Uniform 2 Icon" texture="%ModDir%/Content/Items/Jobgear/OutfitIcons.png" sourcerect="384,0,128,99" origin="0.5,0.5" />
        </add>
        <replace sel="./Wearable/sprite/@texture">engineer_2.png</replace>
        <replace sel="./Wearable/sprite/@hidelimb">false</replace>
        <replace sel="./Wearable/sprite[@limb='RightHand' or @limb='LeftHand' or @limb='RightForearm' or @limb='LeftForearm' or @limb='Waist' or @limb='LeftFoot' or @limb='RightFoot']/@hidelimb">
          true
        </replace>
      </inherit>
    </Item>
  </Override>
</Items>
